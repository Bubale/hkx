{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "firstName": {
      "default": "firstName",
      "maxLength": 100,
      "type": "string",
      "required": true
    },
    "lastName": {
      "default": "lastName",
      "maxLength": 100,
      "type": "string",
      "required": true
    },
    "age": {
      "default": "",
      "type": "integer",
      "required": true
    },
    "briefDescription": {
      "default": "Enter a brief description of your self",
      "maxLength": 500,
      "type": "string"
    },
    "school": {
      "default": "school",
      "maxLength": 100,
      "type": "string",
      "required": true
    },
    "promotion": {
      "default": "promotion",
      "maxLength": 100,
      "type": "string",
      "required": true
    },
    "questions": {
      "collection": "question",
      "via": "user"
    }
  },
  "collectionName": "users-permissions_user"
}